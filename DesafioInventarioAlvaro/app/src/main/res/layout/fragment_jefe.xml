<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/Theme.DesafioInventarioAlvaro.Fondo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JefeFragment" >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/btnGestionarAulas"
            style="@style/Theme.DesafioInventarioAlvaro.Boton"
            android:layout_width="180dp"
            android:layout_height="51dp"
            android:text="@string/GestionarAulas"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/btnGestionarProfesores"
            style="@style/Theme.DesafioInventarioAlvaro.Boton"
            android:layout_width="180dp"
            android:layout_height="51dp"
            android:text="@string/GestionarProfesores"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.222" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>